				{
					"name": "Register User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"",
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"const resJson = pm.response.json();",
									"pm.test(\"Response must have user data\", function () {",
									"    pm.expect(resJson.data).have.property(\"name\", \"user1\");",
									"});",
									"",
									"pm.test(\"Response must have token\", function () {",
									"    pm.expect(resJson.token).to.not.be.undefined;",
									"});",
									"",
									"if (pm.response.code === 201) {",
									"    pm.execution.setNextRequest(\"Register Duplicated User\");",
									"    pm.environment.set(\"USER_TOKEN\", resJson.token);",
									"} else {",
									"    pm.execution.setNextRequest(null);",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
